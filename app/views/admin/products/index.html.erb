<div class="container">
    <div class="row">
        <div class="col-xs-5">
            <div clas="">
                商品一覧
            </div>
            <table >
                <tr>
                    <th>商品ID</th>
                    <th>商品名</th>
                    <th>税抜き価格</th>
                    <th>ジャンル</th>
                    <th>ステータス</th>
                </tr>
                <%= @products.each do |product| %>
                <tr>
                    <td>product.id</td>
                    <td>product.name</td>
                    <td>product.price</td>
                    <td>product.genre_id</td>
                    <td>product.is_active</td>
                </tr>
                <% end %>
            </table>
            
            
            
            
        </div>
    </div>
</div>